{
  "family": "deploy-check-task",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",  // Required for Fargate tasks
  "cpu": "256",  // Task-level CPU
  "memory": "512",  // Task-level memory
  "containerDefinitions": [
    {
      "name": "deploy-check-container",
      "image": "084375547400.dkr.ecr.ap-south-1.amazonaws.com/dep-repo:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
